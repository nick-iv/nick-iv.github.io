include ../includes/footer_menu
extends ../includes/layout

block title
    | Московские закоулки — Nick Iv


block head
        meta(property="og:title",        content="Фотопроект «Московские закоулки»")
        meta(property="og:description",  content="Необычные места Москвы, в&nbsp;которых не&nbsp;обязан побывать каждый Москвич")
        meta(property="og:url",          content="https://nick-iv.me/moscow-backstreets/")
        meta(property="og:image",        content="https://nick-iv.me/static/images/smm-mb.png")
        meta(property="og:image:type",   content="image/png")
        meta(property="og:image:width",  content="398")
        meta(property="og:image:height", content="208")

        script(type="text/javascript", src="//yandex.st/jquery/1.8.1/jquery.min.js")
        script(type="text/javascript", src="//api-maps.yandex.ru/2.1.4/?load=package.full&lang=ru-RU")
        script(type="text/javascript", src="/static/js/mb-map.js")

block body
    body.bLayout.bPage.bMCPage
        .bPage__eWrapper
            header.bPage__eHeader
                span.bPage__eHeaderItem
                    img.bNavi__eAvatar(src="/static/images/ava-mb.svg", alt="")

                span.bPage__eHeaderItem
                    h1.bPage__eTitle Московские закоулки
                    span &nbsp;/&nbsp;
                    a(href="/") На&nbsp;главную

            article.bPage__eArticle
                p.bPage__eParagraph <a href="/moscow-backstreets/about.html">Проект</a> о разных необычные места Москвы, в&nbsp;которых не&nbsp;обязан побывать каждый Москвич.
                div.bMCPage__eMap#YMapsID

                .bMCPage__eList
                    a.bMCPage__eItem(href="/moscow-backstreets/vvedenskoye-cemetery.html")
                        img.bMCPage__eItemImg(src="//img-fotki.yandex.ru/get/3504/nim579.8/0_bc28_79072fde_M.jpg")
                        span.bMCPage__eItemText Введенское кладбище

                    a.bMCPage__eItem(href="/moscow-backstreets/diversion-canal.html")
                        img.bMCPage__eItemImg(src="//img-fotki.yandex.ru/get/5201/30973186.22/0_c8ab3_3adbdb58_M")
                        span.bMCPage__eItemText Деривационный канал

                    a.bMCPage__eItem(href="/moscow-backstreets/zhivopisnyj-bridge.html")
                        img.bMCPage__eItemImg(src="//img-fotki.yandex.ru/get/6109/30973186.12/0_859c4_a820b611_M.jpg")
                        span.bMCPage__eItemText Живописный мост

                    a.bMCPage__eItem(href="/moscow-backstreets/embankment-of-neskuchny-garden.html")
                        img.bMCPage__eItemImg(src="//img-fotki.yandex.ru/get/3607/nim579.b/0_bc97_1c939499_M")
                        span.bMCPage__eItemText Набережная Нескучного Сада

                    a.bMCPage__eItem(href="/moscow-backstreets/kuzminki.html")
                        img.bMCPage__eItemImg(src="//img-fotki.yandex.ru/get/6108/30973186.10/0_8591b_df5853f8_M")
                        span.bMCPage__eItemText Кузьминки

                    a.bMCPage__eItem(href="/moscow-backstreets/novospassky-pond.html")
                        img.bMCPage__eItemImg(src="//img-fotki.yandex.ru/get/5802/30973186.13/0_89869_32a9862f_M")
                        span.bMCPage__eItemText Новоспасский пруд и&nbsp;монастырь

                    a.bMCPage__eItem(href="/moscow-backstreets/serebryany-bor.html")
                        img.bMCPage__eItemImg(src="//img-fotki.yandex.ru/get/6301/30973186.11/0_859bd_d2e248bb_M")
                        span.bMCPage__eItemText Серебряный бор

                    a.bMCPage__eItem(href="/moscow-backstreets/floodplain-of-strogino.html")
                        img.bMCPage__eItemImg(src="//img-fotki.yandex.ru/get/17840/30973186.21/0_bf600_51b013ac_M")
                        span.bMCPage__eItemText Строгинская пойма

                    a.bMCPage__eItem(href="/moscow-backstreets/metro-bridge.html")
                        img.bMCPage__eItemImg(src="//img-fotki.yandex.ru/get/3604/nim579.a/0_bc6f_b723febc_M")
                        span.bMCPage__eItemText Метромост на&nbsp;Воробьевых горах

                    a.bMCPage__eItem(href="/moscow-backstreets/khodynka-field.html")
                        img.bMCPage__eItemImg(src="//img-fotki.yandex.ru/get/16142/30973186.21/0_bf607_660433e8_M")
                        span.bMCPage__eItemText Ходынское поле

                    a.bMCPage__eItem(href="/moscow-backstreets/tatarovskaye-lake.html")
                        img.bMCPage__eItemImg(src="//img-fotki.yandex.ru/get/3104/nim579.0/0_32f9_344e195c_M")
                        span.bMCPage__eItemText Татаровское озеро

                    a.bMCPage__eItem(href="/moscow-backstreets/floodplain-of-nagatino.html")
                        img.bMCPage__eItemImg(src="//img-fotki.yandex.ru/get/6300/30973186.10/0_85738_9c6fa75d_M")
                        span.bMCPage__eItemText Нагатинская пойма
                    a.bMCPage__eItem(href="/moscow-backstreets/abandoned-infectious-buildings.html")
                        img.bMCPage__eItemImg(src="//img-fotki.yandex.ru/get/3001/nim579.1/0_331c_31c628c0_M")
                        span.bMCPage__eItemText Заброшенные инфекционные корпуса

                    a.bMCPage__eItem(href="/moscow-backstreets/petrovsky-palace.html")
                        img.bMCPage__eItemImg(src="//img-fotki.yandex.ru/get/15581/30973186.22/0_bf60f_3fe3bdc9_M")
                        span.bMCPage__eItemText Петровский Путевой Дворец

                    a.bMCPage__eItem(href="/moscow-backstreets/blue-tooth.html")
                        img.bMCPage__eItemImg(src="//img-fotki.yandex.ru/get/9058/30973186.20/0_ac0cb_8688b1cf_M")
                        span.bMCPage__eItemText Синий Зуб

                    a.bMCPage__eItem(href="/moscow-backstreets/river-valley-setun.html")
                        img.bMCPage__eItemImg(src="//img-fotki.yandex.ru/get/15543/30973186.22/0_bf688_12febf2a_M")
                        span.bMCPage__eItemText Долина реки Сетунь

                    //- a.bMCPage__eItem(href="http://nim579.ru/blog/2009-06-04-22")
                        img.bMCPage__eItemImg(src="http://nim579.ru/_bl/0/s77859.jpg", alt="")
                        span.bMCPage__eItemText Парк имени 50-летия Октября

                    //- a.bMCPage__eItem(href="/moscow-backstreets/hzb.html")
                        img.bMCPage__eItemImg(src="http://nim579.ru/_bl/0/s71246.jpg", alt="")
                        span.bMCPage__eItemText Ховринская заброшенная больница

                    .bMCPage__eItem.mEmpty
                    .bMCPage__eItem.mEmpty
                    .bMCPage__eItem.mEmpty
                    .bMCPage__eItem.mEmpty
                    .bMCPage__eItem.mEmpty

                p.bPage__eParagraph Вы&nbsp;тоже можете поучавствовать в&nbsp;проекте, или прислать интересное место, которое стоит посетить. Просто напишите на&nbsp;почту: <a href="mailto:backstreets@nick-iv.me">backstreets@nick-iv.me</a>.

        +footerMenu('backstreets')
